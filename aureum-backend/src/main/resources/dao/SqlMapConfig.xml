			alias="CommentQueryCondition" />
		<typeAlias type="com.brave.backend.dao.model.SeqGen" alias="SeqGen" />
	</typeAliases>
	<mappers>
		<mapper resource="dao/mapper/config-dao.xml" />
		<mapper resource="dao/mapper/account-dao.xml" />
		<mapper resource="dao/mapper/user-dao.xml" />
		<mapper resource="dao/mapper/profile-dao.xml" />
		<mapper resource="dao/mapper/msg-dao.xml" />
