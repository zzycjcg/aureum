<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.brave.backend.dao.model.Config" alias="Config" />
		<typeAlias type="com.brave.backend.dao.model.Account" alias="Account" />
		<typeAlias type="com.brave.backend.dao.model.User" alias="User" />
		<typeAlias type="com.brave.backend.dao.model.Profile" alias="Profile" />
		<typeAlias type="com.brave.backend.dao.model.Msg" alias="Msg" />
		<typeAlias type="com.brave.backend.dao.model.MsgQueryCondition"
			alias="MsgQueryCondition" />
		<typeAlias type="com.brave.backend.dao.model.Comment" alias="Comment" />
